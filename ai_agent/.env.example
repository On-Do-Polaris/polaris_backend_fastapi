# SKAX Physical Risk Analyzer - Environment Variables Example
# Copy this file to .env and fill in your actual values

# Environment
ENVIRONMENT=development

# Climate Data API
CLIMATE_API_URL=https://api.climate.example.com
CLIMATE_API_KEY=your_climate_api_key_here

# Geographic Data API
GEOGRAPHIC_API_URL=https://api.geo.example.com
GEOGRAPHIC_API_KEY=your_geographic_api_key_here

# Debug Mode
DEBUG=True

# Logging
LOG_LEVEL=INFO
